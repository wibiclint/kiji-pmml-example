/** Avro record corresponding to R "iris" dataset. */

/** Example avro protocol. */
@namespace("org.kiji.pmml")
protocol MyRecords {
  record OzonePredictor {
    double O3;
    double temp;
    double ibh;
    double ibt;
  }

  record OzonePredicted {
    double Predicted_O3;
  }

  /** Example record. */
  record ExampleRecord {
    string id;
  }
}
